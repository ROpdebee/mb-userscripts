<script>
function initDiscountModal2021() {
    if (!$.cookie('discount-2021-seen') && location.href.search('place-order') == -1) {
        $('#modal-discount-2021').modal('show').on('hidden.bs.modal', function() {
            $.cookie('discount-2021-seen', true, { expires: 1, path: '/' }); 
        });
    }
}

function addDiscount2021(iso) {
    var voucher = '';
    switch(iso) {
        case 'US':
            voucher = 'THIEQGF3GQ5PC6J';
            break;
        case 'ES':
            voucher = '8AC14U7RO974J2B';
            break;
        case 'ZA':
            voucher = 'IN334VYSWLP75OW';
            break;
        case 'FR':
            voucher = 'FMFGO7NA972COFU';
            break;
        case 'DE':
        	voucher = 'QW5E3B71T6PJFO7';
        	break;
    }

    $('#btn-add-discount').addClass('loading').attr('disabled', 'disabled');
    $('#btn-discount-no-thanks').attr('disabled', 'disabled');
    $.ajax({
        url: '/api/1.2/voucher/redeem/?output_type=json&c=' + voucher,
        success: function(response) {
            $('#modal-discount-2021').modal('hide');
            $('#modal-discount-2021-msg-body').text(response.message);
            $('#modal-discount-2021-msg').modal('show');
        }
    });

}
</script>
