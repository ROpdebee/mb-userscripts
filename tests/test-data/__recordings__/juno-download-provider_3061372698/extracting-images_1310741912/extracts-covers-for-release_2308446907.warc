        function papTrackingFunction() {
            var rs = this.readyState; if (rs && (rs != 'complete') && (rs != 'loaded')) return;
            try { PostAffTracker.disableTrackingMethod('F'); PostAffTracker.track();  } catch (e) { }
                    }
