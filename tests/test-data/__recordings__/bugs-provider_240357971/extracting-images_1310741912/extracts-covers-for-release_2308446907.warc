<script>
var mpdrv_cur_page, mpdrv_count;									// 뮤직PD 추천 리뷰 페이징 관련 변수
var pAritistPopFlag = false;

$(document).mousedown(function(e){
	$('.layer.moreStyle').each(function(vvv, object){
		if( $(this).css('display') == 'block' )
		{
			var l_position = $(this).offset();
			l_position.right = parseInt(l_position.left) + ($(this).width());
			l_position.bottom = parseInt(l_position.top) + parseInt($(this).height());

			if( ( l_position.left <= e.pageX && e.pageX <= l_position.right ) && ( l_position.top <= e.pageY && e.pageY <= l_position.bottom ) ) {
			}
			else {
				$(this).hide(); //toggleClass("hidden");
			}
		}
	});
});

$(window.document).ready(function() {

	RecommendClickLog.setDefaultValue();
	RecommendClickLog.setCardType("SIMILAR");
	RecommendClickLog.setServiceType("associated_item");

	sendClickInfoLog = function(location, seed_id, item_type, item_id, target_type, target_id, action_type){
		params = {};
		this.params.location = location;
		this.params.seed_item_id = seed_id;
		this.params.item_type = item_type;
		this.params.item_id = item_id;
		this.params.target_item_type = target_type;
		this.params.target_item_id = target_id;
		this.params.action_type = action_type;
		this.params.sendTrigger = true;

		RecommendClickLog.sendLogParams(params);
	};

	/* 참여아티스트 */
	$('.layer.participatingArtists').css("visibility", "hidden");
	$('.summaryAlbum .info a.btnMoreArtists').click(function(ev) {
		ev.preventDefault();
		$('.layer.participatingArtists').css("visibility", "");

		if(!pAritistPopFlag)
			bugs.scroll.setScrollBar(".participatingArtists .innerScroll", true);

		pAritistPopFlag = true;
	});

	$('a.btnSwitchgear').click(function(ev) {
		ev.preventDefault();

		$target = $($(this).attr("href"));

		if( $target.hasClass("fold") ) {
			$target.removeClass("fold");
			$(this).addClass("open");
			$(this).find('>em').html("접기");
			setTimeout(function() {
				bugs.navigator._arrangeLayout(true);
			}, 300);
		} else {
			$target.addClass("fold");
			$(this).removeClass("open");
			$(this).find('>em').html("펼치기");
			setTimeout(function() {
				bugs.navigator._arrangeLayout(true);
			}, 300);
		}
	});

	// 자동 재생

	setTimeout(function(){
		// 앨범 설명 (설명 공간 높이보다 내용이 적으면 펼치기/접기 버튼 미표시)
		$("a.btnSwitchgear").each(function(){
			if($(this).hasClass("albumDescSwitchgear")) {
				var folder = $(this).closest(".albumDesc").find(".albumContents");
				var cont = folder.find("span");
				if(cont.height() <= folder.height())
					$(this).css("visibility","hidden");
			}
		});
	}, 500);

	/* 구매 */
	$('.btnNormal.buy').click(function(ev) {
		ev.preventDefault();
		$('.layer.buyMusic').toggleClass("hidden");
	});

	// 시작 - 뮤직 PD 추천 리뷰 페이징
	mpdrv_count = 0;
	mpdrv_cur_page = 1;
	$(".reviewMPD .paging a.btnNext").click(function(ev) {
		reloadMusicPDReviewList("next");
	});
	$(".reviewMPD .paging a.btnPrev").click(function(ev) {
		reloadMusicPDReviewList("prev");
	});
	reloadMusicPDReviewList();
	// 끝 - 뮤직 PD 추천 리뷰 페이징

	window._paging_timer = setInterval(function(){
		try{
			if(bugs.paging) {
				clearInterval(window._paging_timer);
				LoadPagings();
			}
		}
		catch(e){}
	}, 100);

	// 연관 앨범 리스트 (불러오기 & 페이징)
	$.get(g_urlMusic + "/album/json/recommend", {id : "4078166", page : 1, row : 35, decorator : "blank"}, function(result) {

		if($.trim(result) == "") {	// 연관 앨범이 없다면
			$(".relatedAlbum").hide();
			return;
		} else {
			$(".relatedAlbum").show();
		}

		$(".relatedAlbum").html(result);

		if($(".relatedAlbum li").size() > 0) {
			var albumRelatedPager = new bugs.paging("relatedAlbum", $(".relatedAlbum li").size(), 5, 1, 1880, [5, 7]);	// 연관 앨범 리스트 공통 페이징
		}
	});

	// 스타일 리스트 팝업
	$('.summaryAlbum .info a.btnStyleView').click(function(ev) {
		ev.preventDefault();
		var h = ev.pageY;
		$('.layer.moreStyle').css({left: ev.pageX  + 8, top: (h -6) +"px"});
		$('.layer.moreStyle').show();
	});

});

function LoadPagings() {

	var albumMvPager = new bugs.paging("albumMvList", 2, 4, 1, 1880, [4, 6]); 								// 뮤직 비디오 리스트 공통 페이징


	if($(".albumSeriesList li").size() > 0) {
		$(".albumSeriesList .paging").show();
		var albumSeriesPager = new bugs.paging("albumSeriesList", $(".albumSeriesList li").size(), 5, 1, 1880, [5, 7]);	// 연관 앨범 리스트 공통 페이징
	}
}


function reloadMusicPDReviewList(direction) {
	if(mpdrv_count > 0) {
		// 모든 컨텐츠 숨기기
		$(".reviewMPD .recommendReview").hide();

		// 현재 페이지 설정
		if(direction == "next")			mpdrv_cur_page++;
		else if(direction == "prev")	mpdrv_cur_page--;

		// 페이징 버튼 설정
		$(".reviewMPD .paging a").show();
		$(".reviewMPD .paging span").hide();
		if(mpdrv_cur_page == 1)	{
			$(".reviewMPD .paging a.btnPrev").hide();
			$(".reviewMPD .paging span.btnPrev").show();
		}
		if(mpdrv_cur_page >= mpdrv_count) {
			$(".reviewMPD .paging a.btnNext").hide();
			$(".reviewMPD .paging span.btnNext").show();
		}

		// 보여질 컨텐츠 출력
		$($(".reviewMPD .recommendReview")[mpdrv_cur_page-1]).show()
	}
}

function send(){
		albumDownlaodEvent();
}


function albumDownlaodEvent(){
	bugs.wiselog.area('S_ab_02_01');
		bugs.ui.showLoginLayer();
}


</script>




	

