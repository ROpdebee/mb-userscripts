bookletDown = function(downType) {
	if(isMelonLogin()){
		if(downType == "" || typeof downType == "undefined"){
			downType = "booklet";
		}

		$.get('/delivery/bookletInfo.json',{pkageId:'10749882', downType : downType})
				.done(function(data) {
					if(data.RMSG == "SUCCESS") {
						var v = [];
						v = data.bookletInfo;
						if ( v.allHold ) {
							var $iFrm = $('<IFRAME id="iFrm" frameBorder="0" name="iFrm" scrolling="no"></IFRAME>');
							$iFrm.appendTo('body');
							$("#iFrm").attr("src",v.dlBookletUrl);
						} else {
							$('.song_count').html('<strong>' + v.needsCnt + '</strong>/<em>' + v.totalCnt + '</em>곡');
							$('#_booklet').modal('show');
						}
					} else {
						var downTypeStr = "북클릿";
						if(downType != 'booklet'){
							downTypeStr = "영상";
						}

						alert(downTypeStr + " 다운로드 중 에러가 발생했습니다.");
						return;
					}
				});
	} else {
		MELON.WEBSVC.POC.login.loginCheckFuncCall();
	}
}
</script>

