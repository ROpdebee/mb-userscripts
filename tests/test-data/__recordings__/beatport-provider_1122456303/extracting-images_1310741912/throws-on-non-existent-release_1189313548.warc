          <%= hasItem ? "<svg viewBox=\"0 0 200 200\" class=\"cart-menu-in-cart-icon\"><use xlink:href=\"/static/d45efc74b5b2785e43f15dc0fc37303a.svg#icon-in-cart\"></use></svg>" : "" %>
        </span>
        <br />
        <span class="quantity-list"><%= quantity %> <%= quantityLabel %></span>
      </div>
      <div>
        <div class="price" style="max-height: 50%;"><%= price %></div>
      </div>
    </div>
  <%= (header || cartHeader) ? "</a>" : "" %>
</li>
</script>

        
